{
  "window.restoreWindows": "preserve",
  "files.hotExit": "onExit",
  "workbench.startupEditor": "none",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.pl": "prolog"
  },
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/.vscode": true,
    "**/*.meta": true
  },
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontWeight": "800",
  "workbench.colorTheme": "Bearded Theme Coffee Reversed",
  "editor.codeLensFontFamily": "JetBrains Mono",
  "liveServer.settings.donotShowInfoMsg": true,
  "git.enableSmartCommit": true,
  "editor.accessibilitySupport": "off",
  "workbench.colorCustomizations": {
    "editorGutter.background": "#0f0f0f",
    "terminal.foreground": "#ffffff",
    "terminal.background": "#0f0f0f",
    "input.background": "#191919",
    "list.activeSelectionBackground": "#191919",
    "list.hoverBackground": "#191919",
    "editorWidget.background": "#191919",
    "panel.background": "#131313",
    "titleBar.activeBackground": "#131313",
    "titleBar.inactiveBackground": "#131313",
    "breadcrumbPicker.background": "#191919",
    "settings.dropdownBackground": "#191919",
    "settings.numberInputBackground": "#191919",
    "settings.textInputBackground": "#191919",
    "settings.checkboxBackground": "#191919",
    "editor.background": "#0f0f0f",
    "tab.activeBackground": "#131313",
    "tab.inactiveBackground": "#161616",
    "sideBar.background": "#131313",
    "sideBarSectionHeader.background": "#131313",
    "activityBar.background": "#131313",
    "statusBar.background": "#131313",
    "scrollbarSlider.activeBackground": "#131313",
    "tab.activeBorder": "#0f0f0f00",
    "editorGroupHeader.tabsBackground": "#0f0f0f",
    "statusBar.noFolderBackground": "#0f0f0f",
    "tab.border": "#0f0f0f",
    "editorHoverWidget.background": "#0f0f0f",
    "menu.background": "#0f0f0f",
    "debugToolBar.background": "#0f0f0f",
    "editorBracketMatch.background": "#0f0f0f",
    "quickInput.background": "#0f0f0f",
    "editorOverviewRuler.border": "#0f0f0f",
    "notifications.background": "#0f0f0f",
    "breadcrumb.background": "#0f0f0f"
  },
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": true,
  "liveshare.presence": true,
  "git.confirmSync": false,
  "java.autobuild.enabled": false,
  "java.format.enabled": false,
  "java.format.onType.enabled": false,
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.python"
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "editor.formatOnSave": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "liveServer.settings.donotVerifyTags": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-dark",
  "editor.fontSize": 15,
  "editor.cursorSmoothCaretAnimation": "on",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.openRepositoryInParentFolders": "never",
  "window.titleBarStyle": "custom",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.productIconTheme": "icons-carbon"
}
